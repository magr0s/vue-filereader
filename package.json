{
  "name": "vue-filereader",
  "description": "FileReader component for Vue.js",
  "version": "0.1.0",
  "private": false,
  "main": "dist/vue-filereader.common.js",
  "unpkg": "dist/vue-filereader.umd.js",
  "files": [
    "dist/*",
    "*.md"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build:lib": "vue-cli-service build --target lib --name vue-filereader src/components/index.js",
    "lint": "vue-cli-service lint",
    "prepublish": "npm run lint && npm run compile",
    "compile": "npm run build:lib",
    "publish": "npm publish",
    "deploy": "git push"
  },
  "dependencies": {
    "vue": "^2.6.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.5.21"
  },
  "author": "magr0s <magr0s.ak@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/magr0s/vue-filereader/issues"
  },
  "homepage": "https://github.com/magr0s/vue-filereader#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/magr0s/vue-filereader.git"
  },
  "keywords": [
    "filereader",
    "vue"
  ]
}
